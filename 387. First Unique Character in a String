class Solution {
public:
    int firstUniqChar(string s) {
        if(s.length()==0) return -1;
        map<char,int> mp;
        for(int i=0; i<s.length(); i++){
            mp[s[i]]++;
        }
        for(int i=0; i<s.length(); i++){
            if(mp.find(s[i])!=mp.end() && mp[s[i]]==1){
                return i;
                break;
            }
        }
       return -1;    
    }
};
